<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="NHibernate.Test"
                   namespace="NHibernate.Test.NHSpecificTest.GH1550">
  <class name="AbstractClass" table="Derivatives">
    <composite-id>
      <key-many-to-one name="IdOne" class="KeyClass" foreign-key="FK_IdOne" lazy="false"/>
      <key-many-to-one name="IdTwo" class="KeyClass" foreign-key="FK_IdTwo" lazy="false"/>
    </composite-id>
    <discriminator type="String">
      <column name="Type"/>
    </discriminator>
    <subclass name="DerivativeOne" discriminator-value="1"/>
    <subclass name="DerivativeTwo" discriminator-value="2"/>
  </class>
  <class name="KeyClass" table="Keys">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
  </class>
</hibernate-mapping>
